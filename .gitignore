class Alumno:
    # agregarle nombre y nota
    def inicializar(self, nombre, nota):
        self.nombre = nombre
        self.nota = nota

    # imprimir el nombre y la nota
    def imprimir(self):
        print("Nombre: ",self.nombre)
        print("Nota: ",self.nota)
    

    #mensaje de si esta aprovado o no
    def resultado(self):
        if self.nota > 5:
            print("Aprovado con ", self.nota)
        else:
            print("Reprobado con", self.nota)

joao = Alumno()
michelle = Alumno()
Pino = Alumno() 

joao.inicializar("joao", 10)
michelle.inicializar("michelle", 8)
Pino.inicializar("Pino",3)
joao.imprimir()
joao.resultado()
michelle.imprimir()
michelle.resultado()
Pino.imprimir()
Pino.resultado()
